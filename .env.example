# .env.example
# Copy this file to .env and fill in your secrets

# Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# AI Providers - Frontend (VITE_* vars accessible in browser)
# At least one required for AI Playground and real-time features
# OpenAI: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=
# Anthropic: https://console.anthropic.com/account/keys
VITE_ANTHROPIC_API_KEY=
# Google Gemini: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=

# AI Providers - Backend (for Supabase Edge Functions)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=

# Add any other required environment variables here
